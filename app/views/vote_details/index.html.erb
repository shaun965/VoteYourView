<p id="notice"><%= notice %></p>

<h1>Listing Vote Details</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vote_details.each do |vote_detail| %>
      <tr>
        <td><%= link_to 'Show', vote_detail %></td>
        <td><%= link_to 'Edit', edit_vote_detail_path(vote_detail) %></td>
        <td><%= link_to 'Destroy', vote_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vote detail', new_vote_detail_path %>
